        "\n",
        "\n",
        "#Array de pe칞as das bordas\n",
        "arrPecasBordas = np.array(['1,1','1,2','1,3','1,4','1,5','2,1','2,5','3,1','3,5','4,1','4,5','5,1','5,2','5,3','5,4','5,5'])\n",
        "\n",
        "\n",
        "#Fun칞칚o para imprimir\n",
        "def imprimirTabuleiro(arr):\n",
        "      return print(\"| \" + arr[0] + \" | \" + arr[1] + \" | \" + arr[2]+ \" | \" + arr[3] + \" | \" + arr[4] + \" | \" + \"\\n\" +\n",
        "      \"| \" + arr[5] + \" | \" + arr[6] + \" | \" + arr[7]+ \" | \" + arr[8] + \" | \" + arr[9] + \" | \" + \"\\n\" +\n",
        "      \"| \" + arr[10] + \" | \" + arr[11] + \" | \" + arr[12]+ \" | \" + arr[13] + \" | \" + arr[14] + \" | \" + \"\\n\" +\n",
        "      \"| \" + arr[15] + \" | \" + arr[16] + \" | \" + arr[17]+ \" | \" + arr[18] + \" | \" + arr[19] + \" | \" + \"\\n\" +\n",
        "      \"| \" + arr[20] + \" | \" + arr[21] + \" | \" + arr[22]+ \" | \" + arr[23] + \" | \" + arr[24] + \" | \" + \"\\n\")\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "#Fun칞칚o para pegar pe칞a\n",
        "def pegarPeca(arr, posPeca): #Recebe o arrya e o local de onde o jogador quer retirar a pe칞a (LinhaXColuna)\n",
        "  array = np.array(arr) #C칩pia do array original\n",
        "  if posPeca in arrPecasBordas: #Verifica se a pe칞a 칠 uma pe칞a da borda\n",
        "      indice = dicio[posPeca] #Ve no dicionario qual o indice no array corresponde a linha e a coluna retirada\n",
        "      array[indice] = \"仇듚" #Coloca o s칤mbolo na posi칞칚o que foi retirada\n",
        "      imprimirTabuleiro(array) #Imprime tabuleiro com a pe칞a faltando\n",
        "  else: #Caso n칚o seja uma pe칞a v치lida que possa ser retirada\n",
        "    print(\"Pe칞a inv치lida!\") #Exibe mensagem\n",
        "    pegarPeca(arr) #Chama a fun칞칚o de novo\n",
        "\n",
        "\n",
        "def moverElemento(arr, origem, destino, peca):\n",
        "    arr = arr.copy()  # Evita modificar o array original\n",
        "\n",
        "    simbolo = peca  # Pega o valor a ser movido\n",
        "    arr = np.delete(arr, origem)  # Remove o valor da posi칞칚o original\n",
        "    arr = np.insert(arr, destino, simbolo)  # Insere na nova posi칞칚o\n",
        "\n",
        "    return arr\n",
        "\n",
        "\n",
        "def inserirPeca(arr,posTirada,peca): #Insere pe칞a no local desejado\n",
        "\n",
        "  posPeca = input(\"Digite a posi칞칚o onde deseja colocar a pe칞a (LinhaXColuna): \") #Recebe o local onde o jogador quer realocar a pe칞a (LinhaXColuna)\n",
        "  arrMovValidos = dicioMovValidos[posTirada] #Cria um array com os valores do dicion치rio de movimentos v치lidos para aquela pe칞a\n",
        "  posPeca = dicio[posPeca] #Recebe do dicion치rio o valor do indice do array corresponde a posi칞칚o que o jogador deseja inserir\n",
        "  posTirada = dicio[posTirada] #Recebe do dicion치rio o valor do indice do array corresponde a posi칞칚o onde a pe칞a foi retirada\n",
        "  if posPeca in arrMovValidos: #Verifica se a pe칞a que foi retirada pode ser colocada na posi칞칚o que o jogador deseja\n",
        "    arr = moverElemento(arr,posTirada,posPeca, peca) #cria um array novo com a pe칞a no lugar desejado e as outras pe칞as movidas para suas novas posi칞칫es\n",
        "    return arr #Retorna o novo array tabuleiro\n",
        "  else: #Verifica que a pe칞a n칚o pode ser inserida no local que o jogador deseja\n",
        "    print(\"Movimento inv치lido\")\n",
        "    inserirPeca(arr,posTirada) #Chama novamente a fun칞칚o para que o jogador fa칞a uma nova escolha v치lida\n",
        "\n",
        "\n",
        "pontos = 0\n",
        "\n",
